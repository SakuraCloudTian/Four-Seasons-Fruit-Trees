</view>
</view>
<view class="usermsg">
    <view class="msg"> <view class="msg1">种草</view><view class="msg2">{{zhongcao}}</view></view>
    <view class="msg"> <view class="msg1">收藏</view><view class="msg2">{{shoucang}}</view></view>
